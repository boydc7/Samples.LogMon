{
  "environment": "Development",
  "shutdownTimeoutSeconds": 10,

  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },

  "ConfigSettings": {
    "BackoffRetrySeconds": 3,
    "DefaultHttpAccessFile": "/var/log/access.log",
    "MaxRetryTailAttempts": 10,
    "StatsSummaryDisplayInterval": 10,
    "TailInactivityTimeout": 86500,
    "ProcessExistingLogLines": false,
    "TotalTrafficIntervalMinutes": 2,
    "TotalTrafficRequestsPerSecondThreshold": 10
  }
}